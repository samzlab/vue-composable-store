{
  "name": "vue-composable-store",
  "description": "Vue composable store",
  "version": "0.1.3",
  "private": false,
  "author": "Kövesdi György <hello@samzlab.hu>",
  "license": "MIT",
  "module": "dist/index-es.js",
  "main": "dist/index-cjs.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "keywords": [
    "vue",
    "vuex",
    "store"
  ],
  "scripts": {
    "test": "jest --coverage",
    "build:coverage": "make-coverage-badge",
    "build": "rollup -c",
    "prerelease": "npm test && npm run build:coverage && npm run build",
    "release": "standard-version",
    "push": "git push --follow-tags origin master && npm publish"
  },
  "files": [
    "src/*",
    "dist/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@koffeine/eslint-config": "19.1.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-strip": "^2.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-jest": "^26.3.0",
    "eslint": "7.25.0",
    "jest": "^26.4.2",
    "make-coverage-badge": "^1.2.0",
    "rollup": "^2.28.2",
    "rollup-plugin-filesize": "^9.0.2",
    "standard-version": "^9.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "vue": "^3.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-vue": "7.10.0"
  },
  "peerDependencies": {
    "vue": ">3.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "testEnvironment": "jsdom",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "displayName": {
      "name": "Package: vue-composable-store",
      "color": "blue"
    }
  }
}
