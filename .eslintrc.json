{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "@koffeine",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/essential"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
      "no-bitwise": "off",
      "func-style": "off"
    }
}
